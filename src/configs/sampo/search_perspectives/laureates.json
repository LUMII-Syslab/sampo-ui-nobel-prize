{
    "id": "laureates",
    "endpoint": {
        "url": "https://data.nobelprize.org/store/sparql",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesPerspective2.js",
    "baseURI": "http://data.nobelprize.org/resource/laureate/",
    "URITemplate": "<BASE_URI><LOCAL_ID>",
    "facetClass": "nobel:Laureate",
    "frontPageImage": "main_page/works-452x262.jpg",
    "searchMode": "faceted-search",
    "enableDynamicLanguageChange": false,
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "laureates": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "workProperties",
                "sparqlQuery":"laureateWikiDataQuery",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": null
            },
            "instanceConfig": {
                "propertiesQueryBlock": "workProperties",
                "sparqlQuery":"laureateWikiDataQuery",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "laureatesByBirthCountry":{
            "tabID": 1,
            "component": "ApexCharts",
            "tabPath": "laureates_by_country",
            "tabIcon": "ShowChart",
            "sparqlQuery": "laureatesByBirthCountryQuery",
            "facetClass": "laureates",
            "resultMapper":"mapPieChart",
            "seriesTitle": "Laureates born in country",
            "createChartData": "createApexBarChartData",
            "title": "Laureates by country"
        },
        "laureatesCategoryDiagrams":{
            "tabID": 2,
            "component": "ApexCharts",
            "tabPath": "laureates_per_category",
            "tabIcon": "ShowChart",
            "dropdownForResultClasses":true,
            "facetClass": "laureates",
            "defaultResultClass":"laureatesPerCategoryPieChart",
            "resultClasses":
            {
                "laureatesPerCategoryPieChart":
                {
                    "sparqlQuery": "laureatesByCategoryQuery",
                    "resultMapper":"mapPieChart",
                    "createChartData": "createApexPieChartData",
                    "title": "Laureates per category"
                },
                "laureatesPerCategoryTimeline":
                {
                    "sparqlQuery": "laureatesByCategoryTimelineQuery",
                    "resultMapper":"mapDynamicCategoryChart",
                    "resultMapperConfig":
                    {
                        "fillEmptyValues":true
                    },
                    "postprocess":
                    {
                        "func":"convertToCumulativeSumSeries", 
                        "config":{}
                    },
                    "createChartData": "createMultipleLineChartData",
                    "title": "Laureates per category timeline",
                    "xaxisTitle": "Year",
                    "xaxisType": "category",
                    "xaxisTickAmount": 50,
                    "apexChartType":"line",
                    "yaxisTitle": "Laureate count (cumulative sum)",
                    "seriesTitle": "Laureate count",
                    "stroke": {
                        "curve": "straight",
                        "width": 2
                    },
                    "fill": {
                        "type": "solid"
                    }
                }
            }
        },
        "laureateBirthCountryMap":{
            "tabID": 3,
            "component": "LeafletMap",
            "tabPath": "laureate_country_map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "laureateBirthCountryMapQuery",
            "facetClass": "laureates",
            "filterTarget": "laureate",
            "instanceConfig": {
                "propertiesQueryBlock": "laureateBirthCountryInstancePropertyQuery",
                "relatedInstances": "laureateBirthCountryMapRelatedQuery",
                "filterTarget": "related__id",
                "createPopUpContent": "createPopUpContentLaureateBirthCountries"
            }
        },
        "laureatesWithMultiplePrizesBarChart":{
            "tabID": 4,
            "component": "ApexCharts",
            "tabPath": "laureates_with_multiple_prizes_chart",
            "tabIcon": "ShowChart",
            "sparqlQuery": "laureatesWithMultiplePrizesQuery",
            "facetClass": "laureates",
            "resultMapper":"mapPieChart",
            "seriesTitle": "Prizes received",
            "createChartData": "createApexBarChartData",
            "title": "Prize count per laureate"
        },
        "export": {
            "tabID": 5,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "resultClass": "perspective2",
            "facetClass": "perspective2"
        },
        "knowledgeGraphMetadata": {
            "sparqlQuery": "knowledgeGraphMetadataQuery",
            "resultMapper": "makeObjectList"
        }
    },
    "maps":{
        "laureateBirthCountryMap":{
            "center": [22.43, 10.37],
            "zoom": 2
        }
    },
    "datasets":{
        "wikidata":{
            "endpoint":{"url": "https://query.wikidata.org/sparql"}
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id":"laureateImage",
            "valueType":"image",
            "makeLink": false,
            "previewImageHeight":100,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id":"affiliation",
            "valueType":"object",
            "makeLink":true,
            "externalLink":true,
            "sortValues":false,
            "numberedList":false,
            "minWidth":250
        },
        {
            "id": "birthPlaceLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "birthDate",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 150
        },
        {
            "id": "deathDate",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 150
        },
        {
            "id": "foundingDate",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 150
        },
        {
            "id": "foundingLocation",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "gender",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 100
        },
        {
            "id": "fullName",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250,
            "onlyOnInstancePage": true
        }
    ],
    "facets": {
        "prefLabel": {
            "sortByPredicate": "rdfs:label",
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "textQueryProvider": "regex",
            "textQueryProperty": "rdfs:label"
        },
        "affiliation": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "dbo:affiliation",
            "facetLabelPredicate":"rdfs:label",
            "searchField": true,
            "sortButton": false,
            "sortBy": "instanceCount",
            "sortDirection": "desc"
        },
        "gender": {
            "containerClass": "three",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "foaf:gender",
            "labelPattern":"VALUES (?id ?prefLabel_) { ('male' 'Male') ('female' 'Female') }",
            "searchField": false,
            "literal":true,
            "sortButton": false,
            "pieChartButton": true,
            "sortBy": "instanceCount",
            "sortDirection": "desc"
        },
        "birthPlaceLabel": {
            "sortByPredicate": "nobel:birthPlaceLabel"
        }
    }
}